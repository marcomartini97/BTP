                                                                                 rdfs:comment "This is the ontology for the first draft about traffic cars" .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project#hasDetectedOnDate
:hasDetectedOnDate rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isDetectedOnDate ;
                   rdfs:domain :ChemicalDetection ;
                   rdfs:range :Date .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project#hasObservedOnDate
:hasObservedOnDate rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isObservedOnDate .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project#isDetectedOnDate
:isDetectedOnDate rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project#isObservedOnDate
:isObservedOnDate rdf:type owl:ObjectProperty ;
                  rdfs:domain :Date ;
                  rdfs:range :VehicleDetection .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project#isOn
:isOn rdf:type owl:ObjectProperty ;
      owl:inverseOf :isPlaced .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/hasDetect
:hasDetect rdf:type owl:ObjectProperty ;
           owl:inverseOf :isDetected ;
           rdfs:domain :ChemicalDetection ;
           rdfs:range :ChemicalElement .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/hasLevel
:hasLevel rdf:type owl:ObjectProperty ;
          rdfs:domain :Coil ;
          rdfs:range :Level .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/hasObserve
:hasObserve rdf:type owl:ObjectProperty ;
            owl:inverseOf :isObserved ;
            rdfs:domain :Coil ;
            rdfs:range :VehicleDetection .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/hasRegister
:hasRegister rdf:type owl:ObjectProperty ;
             owl:inverseOf :isRegistered ;
             rdfs:domain :PollutionStation ;
             rdfs:range :ChemicalDetection .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/hasType
:hasType rdf:type owl:ObjectProperty ;
         rdfs:domain :Coil ;
         rdfs:range :Type .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/isDetected
:isDetected rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/isNearTo
:isNearTo rdf:type owl:ObjectProperty ;
          rdfs:domain :PollutionStation ;
          rdfs:range :PollutionCoil .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/isObserved
:isObserved rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/isPlaced
:isPlaced rdf:type owl:ObjectProperty ;
          rdfs:domain :Road ;
          rdfs:range :Coil .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/isRegistered
:isRegistered rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/onDay
:onDay rdf:type owl:ObjectProperty ;
       rdfs:domain :Date ;
       rdfs:range :DayWeek .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project#inQuantity
:inQuantity rdf:type owl:DatatypeProperty ;
            rdfs:domain :ChemicalDetection ;
            rdfs:range xsd:float .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/fromDate
:fromDate rdf:type owl:DatatypeProperty ;
          rdfs:domain :Date ;
          rdfs:range xsd:dateTime ;
          rdfs:comment "it uses dateTime as value -> check ranges and how to use it!" .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/hasAccuracy
:hasAccuracy rdf:type owl:DatatypeProperty ;
             rdfs:domain :VehicleDetection ;
             rdfs:range xsd:float ;
             rdfs:comment "NOTE: the value of the accuracy is rappresented using a float" .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/hasChemicalName
:hasChemicalName rdf:type owl:DatatypeProperty ;
                 rdfs:domain :ChemicalDetection ;
                 rdfs:range xsd:string .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/hasCode
:hasCode rdf:type owl:DatatypeProperty ;
         rdfs:domain :Coil ;
         rdfs:range xsd:string .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/hasCount
:hasCount rdf:type owl:DatatypeProperty ;
          rdfs:domain :VehicleDetection ;
          rdfs:range xsd:integer .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/hasID
:hasID rdf:type owl:DatatypeProperty ;
       rdfs:domain :Coil ;
       rdfs:range xsd:string .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/toDate
:toDate rdf:type owl:DatatypeProperty ;
        rdfs:domain :Date ;
        rdfs:range xsd:dateTime .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/ChemicalDetection
:ChemicalDetection rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasDetectedOnDate ;
                                     owl:someValuesFrom :Date
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasDetect ;
                                     owl:someValuesFrom :ChemicalElement
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :isRegistered ;
                                     owl:someValuesFrom :PollutionStation
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :inQuantity ;
                                     owl:someValuesFrom xsd:float
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasChemicalName ;
                                     owl:someValuesFrom xsd:string
                                   ] ;
                   rdfs:comment "Class that rappresents a Chemical Detection (a contamination of a Chemical Agent)" .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/ChemicalElement
:ChemicalElement rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Class ;
                                       owl:oneOf ( :C6H6
                                                   :CO
                                                   :NO
                                                   :NO2
                                                   :NOX
                                                   :O3
                                                   :PM10
                                                   :PM2.5
                                                 )
                                     ] ;
                 rdfs:comment "Class that rappresents a chemical agent"@en .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/Coil
:Coil rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :isOn ;
                        owl:someValuesFrom :Road
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasLevel ;
                        owl:someValuesFrom :Level
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasType ;
                        owl:someValuesFrom :Type
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasCode ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasID ;
                        owl:someValuesFrom xsd:string
                      ] ;
      rdfs:comment "Class that rappresents the machine that register the flow of vehicles"@en .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/Date
:Date rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :onDay ;
                        owl:someValuesFrom :DayWeek
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :fromDate ;
                        owl:someValuesFrom xsd:dateTime
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :toDate ;
                        owl:someValuesFrom xsd:dateTime
                      ] ;
      rdfs:comment "Class that rappresent a moment"@en .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/DayWeek
:DayWeek rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:oneOf ( :Domenica
                                           :Giovedì
                                           :Lunedì
                                           :Martedì
                                           :Mercoledì
                                           :Sabato
                                           :Venerdì
                                         )
                             ] .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/Level
:Level rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Class ;
                             owl:oneOf ( :Level1
                                         :Level2
                                       )
                           ] .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/PollutionCoil
:PollutionCoil rdf:type owl:Class ;
               rdfs:subClassOf :Coil ;
               rdfs:comment "Class for Coil near to a PollutionStation"@en .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/PollutionStation
:PollutionStation rdf:type owl:Class ;
                  rdfs:comment "Class that rappresents a pollution station"@en .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/Road
:Road rdf:type owl:Class .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/Type
:Type rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Class ;
                            owl:oneOf ( :Spira
                                        :Telecamera
                                      )
                          ] .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/VehicleDetection
:VehicleDetection rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasObservedOnDate ;
                                    owl:someValuesFrom :Date
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :isObserved ;
                                    owl:someValuesFrom :Coil
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasAccuracy ;
                                    owl:someValuesFrom xsd:float
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasCount ;
                                    owl:someValuesFrom xsd:integer
                                  ] .


