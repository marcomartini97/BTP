#################################################################

###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/fromDate
:fromDate rdf:type owl:DatatypeProperty ;
          rdfs:domain :Date ;
          rdfs:range xsd:dateTime ;
          rdfs:comment "it uses dateTime as value -> check ranges and how to use it!" .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/hasAccuracy
:hasAccuracy rdf:type owl:DatatypeProperty ;
             rdfs:domain :VehicleDetection ;
             rdfs:range xsd:float ;
             rdfs:comment "NOTE: the value of the accuracy is rappresented using a float" .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/hasChemicalName
:hasChemicalName rdf:type owl:DatatypeProperty ;
                 rdfs:domain :ChemicalDetection ;
                 rdfs:range xsd:string .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/hasCode
:hasCode rdf:type owl:DatatypeProperty ;
         rdfs:domain :Coil ;
         rdfs:range xsd:string .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/hasCount
:hasCount rdf:type owl:DatatypeProperty ;
          rdfs:domain :VehicleDetection ;
          rdfs:range xsd:integer .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/hasID
:hasID rdf:type owl:DatatypeProperty ;
       rdfs:domain :Coil ;
       rdfs:range xsd:string .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/inQuantity
:inQuantity rdf:type owl:DatatypeProperty ;
            rdfs:domain :ChemicalDetection ;
            rdfs:range xsd:float .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/toDate
:toDate rdf:type owl:DatatypeProperty ;
        rdfs:domain :Date ;
        rdfs:range xsd:dateTime .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project#PollutionStationLocation
:PollutionStationLocation rdf:type owl:Class ;
                          owl:equivalentClass [ rdf:type owl:Class ;
                                                owl:oneOf ( :GiardiniMargherita
                                                            :PortaSanFelice
                                                            :ViaChiarini
                                                          )
                                              ] ;
                          rdfs:comment "An enum class that list all the Pollution Station by Street"@en ;
                          rdfs:label "Pollution Station Location"@en .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/ChemicalDetection
:ChemicalDetection rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasDetect ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass :ChemicalElement
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasDetectedOnDate ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass :Date
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :isRegistered ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass :PollutionStation
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasChemicalName ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onDataRange xsd:string
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :inQuantity ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onDataRange xsd:float
                                       ] ;
                   owl:disjointWith :VehicleDetection ;
                   rdfs:comment "Class that rappresents the detection of a chemical agent. It must have a Pollution Station, a Date, a Chemical Element, a Chemical Name, a Quantity and a Chemical Name (es. CO2 is carbon dioxide)"@en ;
                   rdfs:label "Chemical Detection"@en .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/ChemicalElement
:ChemicalElement rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Class ;
                                       owl:oneOf ( :C6H6
                                                   :CO
                                                   :NO
                                                   :NO2
                                                   :NOX
                                                   :O3
                                                   :PM10
                                                   :PM2.5
                                                 )
                                     ] ;
                 rdfs:comment "Class that rappresents a chemical element such as: CO2, CO, NO, NOX, PM10 and so on"@en ;
                 rdfs:label "Chemical Element"@en .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/Coil
:Coil rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :isOn ;
                        owl:someValuesFrom :Road
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasCode ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasID ;
                        owl:someValuesFrom xsd:string
                      ] ;
      rdfs:comment "Class that rappresents the machine that register the flow of vehicles"@en ;
      rdfs:label "Coil"@en .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/Date
:Date rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :fromDate ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:dateTime
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :toDate ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:dateTime
                          ] ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :onDay ;
                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :DayWeek
                      ] ;
      rdfs:comment "Class that rappresent a event in the time"@en ;
      rdfs:label "Date"@en .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/DayWeek
:DayWeek rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:oneOf ( :Domenica
                                           :Giovedì
                                           :Lunedì
                                           :Martedì
                                           :Mercoledì
                                           :Sabato
                                           :Venerdì
                                         )
                             ] ;
         rdfs:comment "Class that rappresents a day of the week. It's an enum class"@en ;
         rdfs:label "Day of the Week"@en .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/Level
:Level rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Class ;
                             owl:oneOf ( :Level1
                                         :Level2
                                       )
                           ] ;
       rdfs:label "Level"@en .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/PollutionCoil
:PollutionCoil rdf:type owl:Class ;
               rdfs:subClassOf :Coil ;
               rdfs:comment "Class for Coil near to a Pollution Station. Only the instances belonging to this subclass are used to evaluate Pollution"@en ;
               rdfs:label "Pollution Coil"@en .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/PollutionStation
:PollutionStation rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty :isLocatedIn ;
                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass :PollutionStationLocation
                                      ] ;
                  rdfs:comment "Class that rappresents a pollution station"@en ;
                  rdfs:label "Pollution Station"@en .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/Road
:Road rdf:type owl:Class ;
      rdfs:label "Road"@en .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/Type
:Type rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Class ;
                            owl:oneOf ( :Spira
                                        :Telecamera
                                      )
                          ] ;
      rdfs:label "Type"@en .


###  http://www.semanticweb.org/cesco/ontologies/2024/9/draft-ontology-gdb-project/VehicleDetection
:VehicleDetection rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasObservedOnDate ;
                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass :Date
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :isObserved ;
                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass :Coil
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasAccuracy ;
                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onDataRange xsd:float
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasCount ;
                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onDataRange xsd:integer
                                      ] ;
                  rdfs:comment "Class that identifies a detection of a vehicle through a coil. It must be associated to: an Accuracy value (float), a Date, a Coil and a Count (integer)"@en ;
                  rdfs:label "Vehicle Detection"@en .


