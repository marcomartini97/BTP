#################################################################

###  http://www.dei.unipd.it/database2/BTPOntology/Incidente_Stradale
:Incidente_Stradale rdf:type owl:Class ;
                    owl:disjointWith :Quartiere ,
                                     :Zona_Di_Prossimita ;
                    rdfs:comment "Contains all the accident records." .


###  http://www.dei.unipd.it/database2/BTPOntology/Quartiere
:Quartiere rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:oneOf ( :Borgo_Panigale_-_Reno
                                             :Navile
                                             :Porto_-_Saragozza
                                             :San_Donato_-_San_Vitale
                                             :Santo_Stefano
                                             :Savena
                                           )
                               ] ;
           owl:disjointWith :Zona_Di_Prossimita ;
           rdfs:comment "Contains all the districts in Bologna." .


###  http://www.dei.unipd.it/database2/BTPOntology/Zona_Di_Prossimita
:Zona_Di_Prossimita rdf:type owl:Class ;
                    rdfs:comment "Contains all the proximity zones related to each districts in Bologna" .


