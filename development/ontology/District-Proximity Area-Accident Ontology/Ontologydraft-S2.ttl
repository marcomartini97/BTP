###  http://www.dei.unipd.it/database2/BTPOntology/Recorded_In_District
:Recorded_In_District rdf:type owl:ObjectProperty ;
                      owl:inverseOf :Records_Incident ;
                      rdfs:domain :Incidente_Stradale ;
                      rdfs:range :Quartiere .


###  http://www.dei.unipd.it/database2/BTPOntology/Records_Incident
:Records_Incident rdf:type owl:ObjectProperty ;
                  rdfs:domain :Quartiere ;
                  rdfs:range :Incidente_Stradale .


