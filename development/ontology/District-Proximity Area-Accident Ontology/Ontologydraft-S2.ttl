#    Data properties
#################################################################

###  http://www.dei.unipd.it/database2/BTPOntology#Area
:Area rdf:type owl:DatatypeProperty ;
      rdfs:domain [ rdf:type owl:Class ;
                    owl:unionOf ( :Quartiere
                                  :Zona_Di_Prossimita
                                )
                  ] ;
      rdfs:range xsd:decimal .


###  http://www.dei.unipd.it/database2/BTPOntology#Geo_Point
:Geo_Point rdf:type owl:DatatypeProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Quartiere
                                       :Zona_Di_Prossimita
                                     )
                       ] .


###  http://www.dei.unipd.it/database2/BTPOntology#Geo_Shape
:Geo_Shape rdf:type owl:DatatypeProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Quartiere
                                       :Zona_Di_Prossimita
                                     )
                       ] .


