#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.dei.unipd.it/~gdb/ontology/btp/belongsToDistrict
:belongsToDistrict rdf:type owl:ObjectProperty ;
                   owl:inverseOf :containsArea ;
                   rdfs:domain :ProximityArea ;
                   rdfs:range :District ;
                   rdfs:comment "This property links proximity areas to districts" ;
                   rdfs:label "belongs To District"@en .


###  http://www.dei.unipd.it/~gdb/ontology/btp/containsArea
:containsArea rdf:type owl:ObjectProperty ;
              rdfs:domain :District ;
              rdfs:range :ProximityArea ;
              rdfs:comment "This property conects districts to proximity areas. it is the inverse of \"belongs_to\" property"@en ;
              rdfs:label "contains Area"@en .


###  http://www.dei.unipd.it/~gdb/ontology/btp/happendInArea
:happendInArea rdf:type owl:ObjectProperty ;
               rdfs:domain :RoadAccident ;
               rdfs:range :ProximityArea ;
               rdfs:comment "This property connects road accidents to proximity areas"@en ;
               rdfs:label "happened In Area"@en .


###  http://www.dei.unipd.it/~gdb/ontology/btp/hasAccidentYear
:hasAccidentYear rdf:type owl:ObjectProperty ;
                 rdfs:domain :RoadAccident ;
                 rdfs:range :Year ;
                 rdfs:comment "this property is pointing to the year in which the recorded accidents happened"@en ;
                 rdfs:label "has Accident Year"@en .


###  http://www.dei.unipd.it/~gdb/ontology/btp/hasArchFromRoad
:hasArchFromRoad rdf:type owl:ObjectProperty ;
                 owl:inverseOf :isArchFromRoad ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain :Road ;
                 rdfs:range :RoadArch ;
                 rdfs:label "has Arch From Road" .


###  http://www.dei.unipd.it/~gdb/ontology/btp/hasArchToRoad
:hasArchToRoad rdf:type owl:ObjectProperty ;
               owl:inverseOf :isArchToRoad ;
               rdfs:domain :Road ;
               rdfs:range :RoadArch ;
               rdfs:label "has Arch To Road" .


###  http://www.dei.unipd.it/~gdb/ontology/btp/hasDeploymentDate
:hasDeploymentDate rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isDeploymentDate ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain :RoadArch ;
                   rdfs:range :Date ;
                   rdfs:comment "Defines the date when a road arch was deployed"@en ;
                   rdfs:label "has Deployment Date" .


###  http://www.dei.unipd.it/~gdb/ontology/btp/hasDetect
:hasDetect rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           owl:inverseOf :isDetected ;
           rdfs:domain :ChemicalDetection ;
           rdfs:range :ChemicalElement ;
           rdfs:label "has Detect" .


###  http://www.dei.unipd.it/~gdb/ontology/btp/hasDetectedOnPeriod
:hasDetectedOnPeriod rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasPeriod ;
                     owl:inverseOf :isDetectedOnPeriod ;
                     rdfs:domain :ChemicalDetection ;
                     rdfs:range :Period ;
                     rdfs:label "has Detect On Period" .


###  http://www.dei.unipd.it/~gdb/ontology/btp/hasDistrict
:hasDistrict rdf:type owl:ObjectProperty ;
             owl:inverseOf :isDistrict ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Road ;
             rdfs:range :District ;
             rdfs:comment "Defines the District a road is in"@en ;
             rdfs:label "has District" .


###  http://www.dei.unipd.it/~gdb/ontology/btp/hasEndYear
:hasEndYear rdf:type owl:ObjectProperty ;
            owl:inverseOf :isEndYear ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Project30 ;
            rdfs:range :Year ;
            rdfs:comment "The Date when a Project is supposed to end"@en ;
            rdfs:label "has End Year" .


###  http://www.dei.unipd.it/~gdb/ontology/btp/hasImpositionDate
