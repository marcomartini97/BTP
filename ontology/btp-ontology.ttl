###  http://www.dei.unipd.it/~gdb/ontology/btp/Road
:Road rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :hasDistrict ;
                            owl:someValuesFrom :District
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasRoadType ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :RoadType
                          ] ;
      rdfs:comment """A Road present in the City of Bologna.
Some roads can be long and traverse multiple districts"""@en .


###  http://www.dei.unipd.it/~gdb/ontology/btp/RoadAccident
:RoadAccident rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                           owl:onProperty :happendInArea ;
                                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass :ProximityArea
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :recordedInDistrict ;
                                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass :District
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:comment "Contains all the accident records of bologna"@en ;
              rdfs:label "Road Accident"@en .


###  http://www.dei.unipd.it/~gdb/ontology/btp/RoadArch
:RoadArch rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasNode ;
                                                       owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                       owl:onClass :RoadNode
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :isArchFromRoad ;
                                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                       owl:onClass :Road
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :isArchToRoad ;
                                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                       owl:onClass :Road
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:comment "An arch that connects two roads"@en ;
          rdfs:label "Road Arch" .


###  http://www.dei.unipd.it/~gdb/ontology/btp/RoadNode
:RoadNode rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :isNode ;
                                owl:someValuesFrom :RoadArch
                              ] ;
          rdfs:comment "A Road node that connects two or more roads"@en ;
          rdfs:label "Road Node" .


###  http://www.dei.unipd.it/~gdb/ontology/btp/RoadType
:RoadType rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:oneOf ( :roadtype_vicolo
                                            :roadtype_autostrada
                                            :roadtype_corte
                                            :roadtype_galleria
                                            :roadtype_giardino
                                            :roadtype_largo
                                            :roadtype_localita
                                            :roadtype_mura
                                            :roadtype_passaggio
                                            :roadtype_piazza
                                            :roadtype_piazzale
                                            :roadtype_ponte
                                            :roadtype_raccordo
                                            :roadtype_rampa
                                            :roadtype_rotonda
                                            :roadtype_salita
                                            :roadtype_sottopasso
                                            :roadtype_strada
                                            :roadtype_undefined
                                            :roadtype_via
                                            :roadtype_viale
                                          )
                              ] ;
          rdfs:comment """The type of road, enumerated class that can be one of:

AUTOSTRADA
CORTE
GALLERIA
GIARDINO
LARGO
LOCALITA'
MURA
UNDEFINED
PASSAGGIO
PIAZZA
PIAZZALE
PIAZZETTA
PONTE
RACCORDO
RAMPA
ROTONDA
SALITA
SOTTOPASSO
STRADA
TANGENZIALE
VIA
VIALE
VICOLO
VOLTONE"""@en ;
          rdfs:label "Road Type" .


